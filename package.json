{
  "name": "moving-average-wasm",
  "version": "0.2.0",
  "description": "calculate moving averages in WebAssembly",
  "main": "index.js",
  "scripts": {
    "build-js": "wat2js ./index.wat > ./index.wat.js",
    "build-wasm": "wat2wasm --dump-module --debug-names -o ./index.wasm ./index.wat",
    "test": "echo js test && tape ./test.js && echo wasm test && tape ./test.wat.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chiefbiiko/moving-average-wasm.git"
  },
  "keywords": [
    "moving-average",
    "wasm"
  ],
  "author": "Noah Anabiik Schwarz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chiefbiiko/moving-average-wasm/issues"
  },
  "homepage": "https://github.com/chiefbiiko/moving-average-wasm#readme",
  "devDependencies": {
    "tape": "^4.9.1",
    "wat2js": "^2.1.1",
    "wat2wasm": "^1.0.2"
  }
}
